<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.aptech.crm.dao.TradeDao">
	<select id="getById" parameterType="int" resultType="org.aptech.crm.pojo.Trade">
		select * from trade where id = #{id}
	</select>
	
	<select id="getAll" resultType="org.aptech.crm.pojo.Trade">
		select * from trade
	</select>
</mapper>